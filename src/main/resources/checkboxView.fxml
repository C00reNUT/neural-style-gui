<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<?import org.kordamp.ikonli.javafx.FontIcon?>
<fx:root maxWidth="-Infinity" prefHeight="30.0" prefWidth="400.0" spacing="4.0" type="HBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <Pane prefHeight="26.0" prefWidth="110.0" />
      <CheckBox fx:id="value" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Label" HBox.hgrow="ALWAYS" />
        <Button fx:id="resetButton" mnemonicParsing="false">
            <graphic><FontIcon iconLiteral="fas-undo"/></graphic>
        </Button>
    </children>
    <padding>
        <Insets bottom="2.0" left="4.0" right="4.0" top="2.0" />
    </padding>
</fx:root>
