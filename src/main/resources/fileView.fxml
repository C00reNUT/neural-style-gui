    <children>
      <Button fx:id="fileButton" alignment="CENTER_RIGHT" maxWidth="120.0" mnemonicParsing="false" prefWidth="110.0" text="Button" />
      <TextField fx:id="filePathText" editable="false" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <Button fx:id="clearButton" cancelButton="true" layoutX="404.0" layoutY="12.0" mnemonicParsing="false" text="C" />
        <Button fx:id="resetButton" mnemonicParsing="false" text="D" />
    </children>
    <padding>
        <Insets bottom="2.0" left="4.0" right="4.0" top="2.0" />
    </padding>
</fx:root>
